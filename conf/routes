# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

GET     /semiproduct/all            controllers.Semiproducts.list

GET		/semiproduct/add			controllers.Semiproducts.insert

POST	/semiproduct/add			controllers.Semiproducts.insertPost

GET		/semiproduct/:pack/edit		controllers.Semiproducts.update(pack:Int)

POST	/semiproduct/:pack/edit		controllers.Semiproducts.updatePost(pack:Int)

#GET     /semiproduct/:pack         controllers.Semiproducts.details(pack:Int)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

#GET     /semiproduct/	            controllers.Semiproducts.list

GET		/semiproduct/add			controllers.Semiproducts.add

POST	/semiproduct/add			controllers.Semiproducts.save

GET		/semiproduct/:pack/edit		controllers.Semiproducts.edit(pack:Int)

POST	/semiproduct/:pack/edit		controllers.Semiproducts.update(pack:Int)

#GET     /semiproduct/:pack         controllers.Semiproducts.details(pack:Int)

#GET	/partdef/					controllers.PartDefinitions.list

GET		/partdef/add				controllers.PartDefinitions.add

POST	/partdef/add				controllers.PartDefinitions.save

GET		/partdef/:id/edit			controllers.PartDefinitions.edit(id:Int)

POST	/partdef/:id/edit			controllers.PartDefinitions.update(id:Int)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
